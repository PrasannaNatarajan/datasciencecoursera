pollutantmean <- function(directory,pollutant,id = 1:332){
mean <- 0
for(i in id){
fp <- sprintf("%s/%03d.csv",directory,id)
data <- read.csv(fp)
rel_data <- subset(data,!is.na(data[pollutant]),select = pollutant)
mean <- mean + (sum(rel_data)/length(rel_data))
}
mean
}
